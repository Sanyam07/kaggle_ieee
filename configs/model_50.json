{
    "features": [
        "Label_Encoding",
        "Label_Encoding_Nway",
        "Count_Encoding",
        "Count_Encoding_Nway",
        "Numeric",
        "OS_and_Browser",
        "Agg",
        "Matrix_Factorize",
        "Agg_Future"
    ],
    "cv": {
        "method": "GroupKFold",
        "n_splits": 5
    },
    "adversarial_validation": {
        "lgb_model_params": {
            "objective": "binary",
            "metric": "auc",
            "boosting": "gbdt",
            "max_depth": 5,
            "num_leaves": 50,
            "learning_rate": 0.1,
            "colsample_bytree": 0.1,
            "subsample": 0.1,
            "subsample_freq": 1,
            "seed": 71,
            "bagging_seed": 71,
            "feature_fraction_seed": 71,
            "drop_seed": 71,
            "verbose": -1
        },
        "lgb_train_params": {
            "num_boost_round": 1000,
            "early_stopping_rounds": 100,
            "verbose_eval": 100
        }
    },
    "model": {
        "name": "catboost",
        "model_params": {
            "loss_function": "Logloss",
            "eval_metric": "AUC",
            "learning_rate": 0.07,
            "n_estimators": 10000,
            "metric_period": 200,
            "od_wait": 500,
            "depth": 8
        }
    },
    "dataset": {
        "input_directory": "data/input/",
        "intermediate_directory": "data/interim/",
        "feature_directory": "data/features/",
        "output_directory": "data/output/"
    }
}